    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "canvas": "^2.9.1",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.36",
    "get-google-fonts": "^1.2.2",
    "gh-pages": "^3.2.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-esbuild": "^0.10.2",
    "gulp-imagemin": "^8.0.0",
